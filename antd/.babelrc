{
  "presets": [
    "es2015",
    "react"
  ],
  "plugins": [
    "transform-object-assign",
    "transform-object-rest-spread",
    ["transform-runtime", {
      "helpers": false,
      "polyfill": false,
      "regenerator": true,
      "moduleName": "babel-runtime"
    }],
    ["import",{
      "libraryName": "antd",
      "style": "css"
    }]
  ]
}